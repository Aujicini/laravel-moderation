{
    "name": "aujicini/laravel-moderation",
    "description": "A simple content moderation system for Laravel.",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Kyriakidis",
            "email": "hootlex@icloud.com"
        },
        {
            "name": "Nicholas English",
            "email": "oxuwazet@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Aujicini\\Moderation\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aujicini\\Moderation\\Test\\": "./tests",
            "App\\Models\\": "./models"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aujicini\\Moderation\\ModerationServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
